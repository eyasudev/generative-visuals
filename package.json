  },
  "dependencies": {
    "animejs": "^2.2.0",
    "chaikin-smooth": "^1.0.4",
    "clamp": "^1.0.1",
    "convex-hull": "^1.0.3",
    "defined": "^1.0.0",
    "earcut": "^2.1.3",
    "glsl-aastep": "^1.0.1",
    "glsl-noise": "0.0.0",
    "glsl-pi": "^1.0.0",
    "glsl-random": "0.0.5",
    "load-asset": "^1.2.0",
    "new-array": "^1.0.0",
    "normalize-path-scale": "^2.0.0",
    "query-string": "^6.1.0",
    "right-now": "^1.0.0",
    "seed-random": "^2.2.0",
    "simplex-noise": "^2.4.0",
    "svg-mesh-3d": "^1.1.0",
    "three": "^0.92.0",
    "three-buffer-vertex-data": "^1.1.0",
    "unlerp": "^1.0.1",
    "weighted-random": "^0.1.0"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "budo": "^11.2.1",
    "glslify": "^6.1.1",
    "glslify-hex": "^2.1.1",
    "surge": "^0.20.1",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "test": "node test.js",
    "prepublish": "npm run dist",
    "deploy": "npm run build && npm run deploy:upload",
    "deploy:upload": "surge -p ./public -d paint-blobs.surge.sh",
    "build-all": "npm run build && npm run build:lib",
    "build": "browserify ./src/index.js -t glslify | uglifyjs -cm > public/bundle.js",
    "dist": "npm run dist:bundle && npm run dist:min",
    "dist:bundle": "mkdir -p dist/ && browserify ./src/index.js -t glslify --standalone artwork > dist/build.js",
    "dist:min": "uglifyjs ./dist/build.js -mc > dist/build.min.js",
    "start": "budo ./src/index.js:bundle.js --dir public/ --live -- -t glslify"
  },
  "glslify": {
    "transform": [
      "glslify-hex"
    ]
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/mattdesl/jsconf-eu-prototype.git"
  },
  "homepage": "https://github.com/mattdesl/jsconf-eu-prototype",
  "bugs": {
